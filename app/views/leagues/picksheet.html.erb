<% provide(:title, "Picksheet") %>
<% if @league.preshow_week? %>
  <%= render 'leagues/picksheet/preshow' %>
<% elsif @league.finale_week? %>
  <%= render 'leagues/picksheet/finale' %>
<% elsif picksheet_closed? %>
  <%= render 'leagues/picksheet/closed' %>
<% else %>
  <%= render 'leagues/picksheet/normal' %>
<% end %>
